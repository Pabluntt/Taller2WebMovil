<?xml version='1.0' encoding='utf-8'?>
<widget id="com.taller2.webmovil" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Taller 2 Web Móvil</name>
    <description>
        Aplicación móvil que integra APIs de Clima, One Piece y Pokémon
    </description>
    <author email="dev@taller2.com" href="https://github.com/Pabluntt/Taller2WebMovil">
        Equipo Taller 2
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="24" />
    <preference name="android-targetSdkVersion" value="33" />
</widget>
